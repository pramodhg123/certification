        "data": {
          "type": "object",
          "properties": {
            "justworks": {
              "type": "boolean",
              "description": "Status of JustWorks"
            },
            "randompin": {
              "type": "boolean",
              "description": "Status of RandomPin"
            },
            "mfcg": {
              "type": "boolean",
              "description": "Status of ManufacturingCertificate"
            }
          }
        }
      },
      "example": {
        "status": "string",
        "data": "object"
      }
    },
    "updateOTMSupportResponse": {
