    "OTMUpdate": {
      "type": "object",
      "required": [
        "id",
        "justworks",
        "mfCertificate",
        "randompin"
      ],
      "properties": {
        "id": {
          "type": "string",
          "description": "UUID of the Device"
        },
        "justworks": {
          "type": "boolean",
          "description": "True or False"
        },
        "randompin": {
          "type": "boolean",
          "description": "True or False"
        },
        "mfCertificate": {
          "type": "boolean",
          "description": "True or False"
        }
      },
      "example": {
        "id": "id",
        "justworks": true,
        "randompin": true,
        "mfCertificate": true
      }
    },
