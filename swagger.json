  "definitions": {
    "clientStartResponse": {
      "properties": {
        "status": {
          "type": "string",
          "description": "Status of Client-Start Operation - Started or Error"
        }
      },
      "example": {
        "status": "started"
      }
    },
    "clientStopResponse": {
      "properties": {
        "status": {
          "type": "string",
          "description": "Status of Client-Stop Operation - Stopped or Error"
        }
      },
      "example": {
        "status": "status"
      }
    },
    "clientStatusResponse": {
      "properties": {
        "status": {
          "type": "string",
          "description": "Status of OCf-Client - Running or Stopped or Error"
        }
      },
      "example": {
        "status": "Running or Stopped"
      }
    },
    "getVersionResponse": {
      "properties": {
        "toolVersion": {
          "type": "string",
          "description": "IOP Tool Version Used"
        },
        "iotivityVersion": {
          "type": "string",
          "description": "Iotivity Version Used"
        },
        "buildDateTime": {
          "type": "string",
          "description": "Build Date and Time"
        },
        "os": {
          "type": "string",
          "description": "Operating System Used"
        },
        "nodeVersion": {
          "type": "string",
          "description": "Node Version Used"
        },
        "iotivityPatchApplied": {
          "type": "string",
          "description": "Iotivity Patche Applied"
        }
      },
      "example": {
        "toolVersion": "0.8",
        "os": "Linux",
        "iotivityVersion": "1.3.1",
        "buildDateTime": "11-04-2019 16:20",
        "nodeVersion": "8.0",
        "iotivityPatchApplied": "2222"
      }
    },
    "createDeviceResponse": {
      "properties": {
        "status": {
          "type": "string",
          "description": "Status of Create-Device Operation"
        },
        "data": {
          "type": "string",
          "description": "UUID of the Device Created"
        }
      },
      "example": {
        "status": "status",
        "data": "uuid of the device created - 1111-2222-2222-22222"
      }
    },
    "deleteDeviceResponse": {
      "properties": {
        "status": {
          "type": "string",
          "description": "Status of Delete-Device Operation"
        }
      },
      "example": {
        "status": "success or failure"
      }
    },
    "otmResetDeviceResponse": {
      "properties": {
